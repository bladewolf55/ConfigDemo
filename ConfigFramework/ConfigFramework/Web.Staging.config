<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

	<connectionStrings>
		<add name="ConfigDb" connectionString="Server=.;Database=ConfigStaging;Trusted_Connection=True" providerName="System.Data.SqlClient"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
	</connectionStrings>

	<appSettings>
		<add key="TestKey" value="Staging" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
	</appSettings>

</configuration>